{
  "name": "advanced-security-sync",
  "version": "1.0.0",
  "description": "Clean, well-architected Node.js application for handling GitHub code scanning alert webhooks",
  "author": "",
  "license": "MIT",
  "keywords": [
    "github",
    "webhooks",
    "code-scanning",
    "security",
    "clean-architecture"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@octokit/rest": "^20.0.0",
    "dotenv": "^16.4.0",
    "hono": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.30.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.1.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
